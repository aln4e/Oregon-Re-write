<table>
  <thead>
    <th>Name</th>
    <th>Status</th>
    <th>Condition</th>
  </thead>
  <% for(var i=0; i<game.players.length; i++){%>
    <tr>
      <td> <%=game.players[i].name%> </td>
      <td> <%=game.players[i].status%> </td>
      <td> <%=game.players[i].disease%> </td>
    </tr>
  <%}%>
</table>

<table>
  <thead>
    <th>Supply</th>
    <th>Quantity</th>
  </thead>
  <tr>
    <td>oxen</td>
    <td><%= game.supplies.oxen %></td>
  </tr>
  <tr>
    <td>Sets of Clothing</td>
    <td><%= game.supplies.setsClothing %></td>
  </tr>
  <tr>
    <td>Bullets</td>
    <td><%= game.supplies.bullet %></td>
  </tr>
  <tr>
    <td>Wagon Wheels</td>
    <td><%= game.supplies.wagonWheels %></td>
  </tr>
  <tr>
    <td>Wagon Axels</td>
    <td><%= game.supplies.wagonAxels %></td>
  </tr>
  <tr>
    <td>Wagon Tongues</td>
    <td><%= game.supplies.wagonTongues %></td>
  </tr>
  <tr>
    <td>Pounds of Food</td>
    <td><%= game.supplies.poundsFood %></td>
  </tr>
</table>

